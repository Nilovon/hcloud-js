  "repository": {
    "type": "git",
    "url": "https://github.com/nilovonjs/hcloud-js.git"
  },
  "bugs": {
    "url": "https://github.com/nilovonjs/hcloud-js/issues"
  },
  "homepage": "https://hcloud-js.nilovon.com",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "peerDependenciesMeta": {
